{
    "version": 2,
    "builds": [
      {
        "src": "index.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/placas/consulta/:placa", 
        "dest": "index.js" 
      },
      {
        "src": "/api/placas/cadastroPlaca",
        "dest": "index.js"
      },
      {
        "src": "/api/placas/relatorio/cidade/:cidade",
        "dest": "index.js"
      }
    ]
  }
  